{
	"env": {
		"browser": true
	},
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "warn",
		"spaced-comment": "off",
		"indent": ["warn", "tab"],
		"no-use-before-define": "off",
		"no-inner-declarations": "off",
		"no-param-reassign": "warn",
		"radix": "off",
		"no-shadow": "warn",
		"no-plusplus": "off"
	}
}
